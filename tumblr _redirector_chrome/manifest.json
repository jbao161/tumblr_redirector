{

  "description": "redirect Tumblr image urls to large size (1280)",
  "manifest_version": 2,
  "name": "tumblr redirector",
  "version": "1.0",
  "author": "Jonathan Bao",

  "permissions": [
    "webRequest", "webRequestBlocking", 
	"http://*/*", 
    "https://*/*",
	"storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
  "scripts": ["bdsmlr.js"]
}

}